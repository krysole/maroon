fn printf[[i8] string, ...] i32;
fn puts[[i8] string] i32;
fn exit[i32 exit_code] halt;

let a <- array{i64, 10}[];

fn main[] halt {
  let p <- ptr[a[8 - 1]];
  
  a[1 - 1] <- 2;
  a[4 - 1] <- 84;
  a[8 - 1] <- a[4 - 1] quo a[1 - 1];
  
  printf["Hello world with a == %d.\n", ref[p]];
  
  exit[0];
}
