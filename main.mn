--
-- Test program.
--

fn printf[string: ptr{i8}, ...]: i32;
fn puts[string: ptr{i8}]: i32;
fn exit[exit_code: i32]: halt;

fn main[]: halt {
  let a <- array{i64, 10}[];
  let s <- i64[0];
  
  for i <- i64[0], c <- i64[10]; i < c; i <- i + 1 {
    a[i] <- c - i;
  }
  
  for i <- i64[0], c <- i64[10]; i < c; i <- i + 1 {
    printf["a[%d] -> %d\n", i, a[i]];
    s <- s + a[i];
  }
  
  printf["sum of a[...] -> %d\n", s];
  
  exit[0];
}
